{
    "name": "wendelladriel/laravel-hut",
    "description": "A collection of Helpers and Utilities for your Laravel application",
    "type": "library",
    "keywords": ["laravel", "helpers", "utilities"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "WendellAdriel\\LaravelHut\\": "src/"
        }
    },
    "support": {
        "issues": "https://github.com/WendellAdriel/laravel-hut/issues",
        "source": "https://github.com/WendellAdriel/laravel-hut"
    },
    "authors": [
        {
            "name": "Wendell Adriel",
            "email": "wendelladriel.ti@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-curl": "*"
    },
    "require-dev": {
        "illuminate/auth": "^7.0|^8.0|^9.0",
        "illuminate/console": "^7.0|^8.0|^9.0",
        "illuminate/database": "^7.0|^8.0|^9.0",
        "illuminate/http": "^7.0|^8.0|^9.0",
        "illuminate/routing": "^7.0|^8.0|^9.0",
        "illuminate/support": "^7.0|^8.0|^9.0",
        "illuminate/validation": "^7.0|^8.0|^9.0",
        "laravel/pint": "^1.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "WendellAdriel\\LaravelHut\\LaravelHutServiceProvider"
            ],
            "aliases": {
                "LaravelHut": "WendellAdriel\\LaravelHut\\Facades\\LaravelHut"
            }
        }
    }
}
